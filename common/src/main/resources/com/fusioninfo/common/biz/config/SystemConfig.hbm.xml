<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fusioninfo.common.biz.config.SystemConfig" table="sys_config">
		<cache usage="read-write" />
		<id name="id" column="config_id" unsaved-value="null">
			<generator class="assigned" />
		</id>
		<version name="version" column="version" />
		<property name="state"
			type="com.fusioninfo.pojo.type.PojoStateType" />
		<property name="createTime" column="create_time" />
		<property name="updateTime" column="update_time" />
		<many-to-one name="createUser" column="create_user"
			class="com.fusioninfo.common.biz.user.AbstractUser" />
		<many-to-one name="updateUser" column="update_user"
			class="com.fusioninfo.common.biz.user.AbstractUser" />

		<property name="code" />
		<property name="name" />
		<property name="value" />
	</class>
</hibernate-mapping> 